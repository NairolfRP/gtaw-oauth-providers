{
  "name": "@gtaw-oauth-providers/adonisjs-ally",
  "version": "2.0.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "GTA World OAuth provider for AdonisJS Ally.",
  "keywords": [
    "adonisjs",
    "ally",
    "gtaw",
    "provider",
    "oauth",
    "gtaworld"
  ],
  "homepage": "https://github.com/NairolfRP/gtaw-oauth-providers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NairolfRP/gtaw-oauth-providers.git"
  },
  "author": {
    "name": "NairolfRP",
    "url": "https://github.com/NairolfRP"
  },
  "type": "module",
  "files": [
    "*.js",
    "*.d.ts",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "default": "./index.js"
    },
    "./src": {
      "types": "./src/index.d.ts"
    },
    "./types": {
      "types": "./types.d.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf *.js *.d.ts* src"
  },
  "devDependencies": {
    "@adonisjs/ally": "^5.1.1",
    "@adonisjs/core": "^6.19.0",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.4",
    "@adonisjs/tsconfig": "^1.4.0",
    "@swc/core": "1.11.24",
    "ts-node-maintained": "^10.9.5",
    "typescript": "~5.9.2"
  },
  "peerDependencies": {
    "@adonisjs/ally": "^5.1.1",
    "@adonisjs/core": "^6.19.0"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config"
}
